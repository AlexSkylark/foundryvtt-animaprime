<script>        
    $(document).ready(() => {
        $(document).keypress(function(e){            
            if(e.code == "KeyS" && e.ctrlKey && e.shiftKey) {                
                $(".gm-hud-container").toggle();
                $("#gm-customskill-actor").focus();
                $("#gm-customskill-actor").select();
            }
        });        
    });
</script>